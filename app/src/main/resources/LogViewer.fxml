<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>


<GridPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="McLauncher.LogViewerController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <TableView fx:id="table" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="1090.0">
        <columns>
            <TableColumn fx:id="time" editable="false" maxWidth="500.0" minWidth="150.0" prefWidth="150.0" sortType="DESCENDING" sortable="false" text="Time" />
          <TableColumn fx:id="level" editable="false" prefWidth="75.0" sortable="false" text="Level" />
            <TableColumn fx:id="classCol" editable="false" maxWidth="500.0" minWidth="250.0" prefWidth="250.0" sortable="false" text="Class" />
            <TableColumn fx:id="message" editable="false" minWidth="614.0" prefWidth="614.0" sortable="false" text="Message" />
        </columns>
      </TableView>
   </children>
</GridPane>
